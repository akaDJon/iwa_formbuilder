<?xml version='1.0' encoding='utf-8'?>
<forms version="2">

	<form name='main'
	      form_html='true'
	>

		<field_text
			name='text1'
			required='true'
		/>

		<field_text
			name='text2'
		/>

		<field_textinteger
			name='textinteger'
			validate='min:10|max:5000'
		/>

		<field_select
			name='list'
			label='label.list'
			required='true'
		>
			<option value='1'>Значение 1</option>
			<option value='2'>Значение 2</option>
			<option value='3'>Значение 3</option>
		</field_select>

		<field_select
			name='multiple'
			label='Множественный выбор'
			multiple='true'
			required='true'
			validate_messages='require:test'
		>
			<option value='1'>Значение 1</option>
			<option value='2'>Значение 2</option>
			<option value='3'>Значение 3</option>
		</field_select>

		<field_textform
			name='textform1'
			subform_type='embedded'
		/>

		<field_textform
			name='textform2'
			subform_type='independent'
		/>

		<field_submit
			name='submit'
			button_text='Отправить форму'
		/>

	</form>

</forms>
