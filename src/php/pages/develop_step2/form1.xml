<?xml version='1.0' encoding='utf-8'?>
<forms version="2">

	<form name='main'
	      form_html='true'
	>

		<field_hidden
			name='hidden'
			label='скрытое поле'
			description='описание'
		/>

		<field_text
			name='text'
			label='Текстовый input'
			description='описание'
		/>

		<field_textdouble
			name='textdouble'
			label='Два текстовых input'
		/>

		<field_texttriple
			name='texttriple'
			label='Три текстовых input'
		/>

		<field_textform
			name='textform'
			label='Филд-Форма'
		/>

		<field_select
			name='list'
			label='Список'
		>
			<option value='1'>Значение 1</option>
			<option value='2'>Значение 2</option>
			<option value='3'>Значение 3</option>
		</field_select>

		<field_select
			name='multiple'
			label='Множественный выбор'
			multiple='true'
		>
			<option value='1'>Значение 1</option>
			<option value='2'>Значение 2</option>
			<option value='3'>Значение 3</option>
		</field_select>

		<field_submit
			name='submit'
			button_text='Отправить форму'
		/>

	</form>

</forms>
