<?xml version='1.0' encoding='utf-8'?>
<form>
	<!--	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"-->
	<!--	xsi:noNamespaceSchemaLocation="https://iwa_formbuilder/src/xml/form.xsd"-->

	<fieldset name='main'>

		<field_hidden
			name='hidden'
			label='скрытое поле'
			description='описание'
		/>

		<field_text
			name='text'
			label='текстовый input'
			description='описание'
		/>

		<field_textdouble
			name='textdouble'
			label='два текстовых input'
		/>

		<field_texttriple
			name='texttriple'
			label='три текстовых input'
		/>

		<field_textform
			name='textcouple'
			label='два текстовых field'
		/>

		<field_list
			name='list'
			label='С описанием'
		>
			<option value='1'>Значение 1</option>
			<option value='2'>Значение 2</option>
			<option value='3'>Значение 3</option>
		</field_list>

		<field_submit
			name='submit'
			button_text='Отправить форму'
		/>

	</fieldset>

</form>
