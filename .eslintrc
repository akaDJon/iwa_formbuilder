{
	"env": {
		"browser": true,
		"es2021": true
	},
	"parserOptions": {
		"ecmaVersion": 2021,
		"sourceType": "module"
	},
	"plugins": [
		"prettier",
		"unicorn"
	],
	"extends": [
		"airbnb-base",
		"plugin:unicorn/recommended",
		"plugin:prettier/recommended",
		"prettier"
	],
	"rules": {
		"import/prefer-default-export": "off",
		"prettier/prettier": "error",
		"no-debugger": "off",
		"no-console": "off"
	}
}
